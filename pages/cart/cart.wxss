/* pages/cart/cart.wxss */
.num{
  position: fixed;
  bottom:0;
  left: 0;
  width: 100%;
  height: 120rpx;
  border-top: 1px solid #eee;
 display: flex;
 justify-content: space-between;
 align-items: center;
}
.num>view:nth-of-type(1){
  width: 65%;
  padding-left: 20rpx;
  font-size: 14px;
  color: red;
}
.num>view:nth-of-type(2){
  width: 35%;
  background: red;
  color:#fff;
  text-align:center;
  line-height: 120rpx;
}
.del{
  width: 150rpx;
  height:170rpx;
  background: red;
  position: absolute;
  right: -150rpx;
  top:0;
  text-align: center;
  line-height: 170rpx;
}
.box {
    width:100%;
    height: 200rpx;
    /*white-space 不能丢  */
    white-space: nowrap;
    border: 1px solid red;
}
.box-item {
    width: 45%;
    height:100%;
    border:2rpx solid green;
    display: inline-block
}
/* pages/shop/shop.wxss */
.box_shop{
  width: 100%;
  height: 100%;
  position: relative;
   /* background: #eee; */
}
/* 内容区域布局 */
.shop_shop{
  width: 100%;
  height:100%;
  text-align: center;
  position: absolute;
  top: 0;
  left: 0;
  background: #eee;
}
.shop_shop>image{
  width: 80px;
  height: 80px;
  margin-top: 60px;
}
.shop_shop>view{
  color: #ccc;
  margin: 20px 0;
}
.shop_shop>button{
  background: red;
  color: #fff;
}
/* api广告 */
.cha,.cha>image{
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 100px;
}
/* 渲染加入购物车的数据 */
.shop_list{
  padding: 10rpx 0px;
  height: 150rpx;
  /* height: auto; */
  width:100%;
  border-bottom: 1px solid #eee;
  display: flex;
  position: relative;
  /* overflow: ; */
  align-items: center;
}
.shop_list>image{
  width: 60px;
  height: 60px;
}
.shop_list_one{
    display: flex;
    justify-content: space-between;
    width: 80%;
}
.shop_list_one_left{
  font-size: 14px;
  font-weight: bold;
  width: 64%;
  padding: 0 10px;
}
.shop_list_one_left_text{
  width: 100%;
  /* border: 1px solid #000; */
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.shop_list_one_right>view{
  display: flex;
}
.shop_list_one_right .btn{
  width: 60px;
  height: 30px;
  padding: 0;
  margin: 0;
  min-height: 30px;
  background: red;
  color: #fff;
  border: 1px solid #eee;
  /* display: none; */
}
.shop_list_one_right .btn_one{
  width: 20px;
  margin-top:20px;
  background: #fff;
  color: #000;
}
.shop_list_one_right_num{
  width: 20px;
  height: 30px;
  line-height: 30px;
  background: #ffff;
   margin-top:20px;
   text-align: center;
   border-top:1px solid #eee;
   border-bottom:1px solid #eee; 
}
.shop_list_one_left_price{
  color: red;
}
.as{
  color: #ccc;
  font-size: 10px;
  margin-right: 6px;
}